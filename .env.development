# Development Environment Configuration
# Copy this file to .env for local development

# Server Configuration
PORT=3000
WS_PORT=8081

# Authentication (Development - Replace with your actual values)
SUPABASE_JWT_SECRET=your-development-jwt-secret

# Anthropic API (Development - Replace with your actual key)
ANTHROPIC_API_KEY=your-development-api-key

# Docker Configuration
DOCKER_IMAGE_NAME=claude-env
DOCKER_CONTAINER_PREFIX=claude-dev-session

# Session Configuration (Development defaults)
SESSION_TIMEOUT_MINUTES=60
MAX_CONCURRENT_SESSIONS=5

# Development flags
NODE_ENV=development
DEBUG=true