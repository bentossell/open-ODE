# Production Environment Configuration
NODE_ENV=production
PORT=3000
WS_PORT=8081

# API Keys (never commit real keys)
ANTHROPIC_API_KEY=your_api_key_here
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# Docker Configuration
DOCKER_IMAGE_NAME=openode-claude-env
DOCKER_REGISTRY=registry.example.com

# Logging
LOG_LEVEL=error

# CORS
ALLOWED_ORIGINS=https://openode.com,https://www.openode.com

# SSL
SSL_CERT_PATH=/etc/ssl/certs/production.crt
SSL_KEY_PATH=/etc/ssl/private/production.key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100