# Development Environment Configuration
# This file contains development-specific environment variables

# Node Environment
NODE_ENV=development
PORT=3000
WS_PORT=8081

# Database Configuration (Development)
DATABASE_URL=postgresql://dev_user:dev_password@localhost:5432/claude_terminal_dev
REDIS_URL=redis://localhost:6379

# Supabase Configuration (Development)
SUPABASE_URL=https://your-dev-project.supabase.co
SUPABASE_ANON_KEY=your-development-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-development-service-role-key

# JWT Configuration
JWT_SECRET=dev-jwt-secret-change-in-production
JWT_EXPIRY=24h

# API Configuration
API_URL=http://localhost:3000
WS_URL=ws://localhost:8081

# Feature Flags
ENABLE_AUTH=true
ENABLE_FILE_UPLOAD=true
MAX_MESSAGE_LENGTH=50000
MAX_WORKSPACE_SIZE=100MB

# Logging
LOG_LEVEL=debug
LOG_FORMAT=dev

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock
DOCKER_TIMEOUT=300000

# CORS Configuration
CORS_ORIGIN=http://localhost:3001

# Session Configuration
SESSION_SECRET=dev-session-secret
SESSION_MAX_AGE=86400000

# Rate Limiting
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX_REQUESTS=100

# Development Specific
DEBUG=true
ENABLE_HOT_RELOAD=true