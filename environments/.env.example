# Example Environment Configuration
# Copy this file to .env and update with your actual values
# DO NOT commit .env files with real credentials to version control

# Node Environment
NODE_ENV=development
PORT=3000
WS_PORT=8081

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/claude_terminal
REDIS_URL=redis://localhost:6379

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# JWT Configuration
JWT_SECRET=your-secret-key-here
JWT_EXPIRY=24h

# API Configuration
API_URL=http://localhost:3000
WS_URL=ws://localhost:8081

# Feature Flags
ENABLE_AUTH=false
ENABLE_FILE_UPLOAD=true
MAX_MESSAGE_LENGTH=50000
MAX_WORKSPACE_SIZE=100MB

# Logging
LOG_LEVEL=debug
LOG_FORMAT=dev

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock
DOCKER_TIMEOUT=300000

# CORS Configuration
CORS_ORIGIN=http://localhost:3001

# Session Configuration
SESSION_SECRET=your-session-secret
SESSION_MAX_AGE=86400000

# Rate Limiting
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX_REQUESTS=100

# Optional: Monitoring (Production)
NEW_RELIC_APP_NAME=claude-terminal
NEW_RELIC_LICENSE_KEY=your-new-relic-key

# Optional: Error Tracking (Production)
SENTRY_DSN=your-sentry-dsn
SENTRY_ENVIRONMENT=development