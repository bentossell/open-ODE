# Client Staging Environment Configuration

# Supabase Configuration (Staging)
REACT_APP_SUPABASE_URL=https://your-staging-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=${STAGING_SUPABASE_ANON_KEY}

# Server Configuration (Staging)
REACT_APP_API_URL=https://staging.claude-terminal.example.com
REACT_APP_WS_URL=wss://staging.claude-terminal.example.com:8081

# Feature Flags (Staging settings)
REACT_APP_ENABLE_AUTH=true
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_MAX_MESSAGE_LENGTH=50000

# Environment
REACT_APP_ENV=staging
REACT_APP_DEBUG=false

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=${STAGING_GA_TRACKING_ID}
REACT_APP_MIXPANEL_TOKEN=${STAGING_MIXPANEL_TOKEN}