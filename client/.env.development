# Client Development Environment Configuration
# Copy this file to .env for local development

# Supabase Configuration (Development - Replace with your actual values)
REACT_APP_SUPABASE_URL=https://your-dev-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-development-anon-key

# Server Configuration (Local development)
REACT_APP_API_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:8081

# Feature Flags (Development settings)
REACT_APP_ENABLE_AUTH=false
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_MAX_MESSAGE_LENGTH=50000

# Development flags
REACT_APP_ENV=development
REACT_APP_DEBUG=true